// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

enum launchesType {
  id
  provider
}

enum eventsType {
  id
  provider
}

model Articles {
  id          Int            @id @default(autoincrement())
  url         String
  title       String
  summary     String
  featured    Boolean
  newsSite    String
  imageUrl    String
  publishedAt String
  launches    launchesType[]
  events      eventsType[]

  @@map("articles")
}
